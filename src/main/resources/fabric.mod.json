{
	"schemaVersion": 1,
	"id": "possessive",
	"version": "${version}",
	"name": "Possessive",
	"description": "A client addon for Armor-Poser mod which allows you to possess the armorstand.",
	"authors": [
		"Just_S"
	],
	"contact": {
		"homepage": "https://boosty.to/justs_js",
		"sources": "https://github.com/JustS-js/possessive"
	},
	"license": "MIT",
	"icon": "assets/possessive/icon.png",
	"environment": "client",
	"entrypoints": {
		"client": [
			"net.just_s.PossessiveModClient"
		]
	},
	"mixins": [
		{
			"config": "possessive.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.16.9",
		"minecraft": ">=1.20.2",
		"java": ">=17",
		"fabric-api": "*",
		"armorposer": ">=3.0.0"
	},
	"accessWidener" : "possessive.accesswidener"
}